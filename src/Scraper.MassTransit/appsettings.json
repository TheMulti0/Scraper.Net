    },
    "Youtube": {
      "Enabled": true,
      "AppName": "Scraper.MassTransit",
      "ApiKey": ""
    },
    "YoutubeDl": {
      "Enabled": false,
      "KeepReceivedPost": false,
      "YoutubeDlPath": "youtube-dl"
    },
    "Screenshot": {
      "Enabled": false,
      "KeepReceivedPost": true,
      "UserId": "",
      "ApiKey": ""
    }
  },
  "RabbitMq": {
    "Enabled": true,
    "ConnectionString": "amqp://guest:guest@rabbitmq//"
  }
