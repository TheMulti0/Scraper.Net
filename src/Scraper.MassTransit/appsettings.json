{
  "Logging": {
    "LogLevel": {
      "Default": "Information",
      "Microsoft": "Warning",
      "Microsoft.Hosting.Lifetime": "Information",
      "Scraper.Net": "Debug"
    }
  },
  "Sentry": {
    "Dsn": "",
    "MinimumBreadcrumbLevel": "Debug",
    "MinimumEventLevel": "Warning",
    "AttachStackTrace": true,
    "Debug": false,
    "DiagnosticsLevel": "Error"
  },
  "Scraper": {
    "Feeds": {
      "Enabled": true
    },
    "Twitter": {
      "Enabled": true,
      "ConsumerKey": "",
      "ConsumerSecret": ""
    },
    "Facebook": {
      "Enabled": true,
